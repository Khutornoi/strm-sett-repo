<keymap version="1" name="Default copy" parent="$default">
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="alt space" />
  </action>
  <action id="LocalHistory.ShowHistory">
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="SmartTypeCompletion">
    <keyboard-shortcut first-keystroke="shift ctrl space" />
    <keyboard-shortcut first-keystroke="shift alt space" />
  </action>
  <action id="Vcs.ShowTabbedFileHistory">
    <keyboard-shortcut first-keystroke="alt g" />
  </action>
</keymap>